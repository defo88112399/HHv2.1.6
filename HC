    139767598,
    1004603668,
    1370994019,
    480203269,
    2972781016,
    4666105438,
    6007169310,
    984607801,
    432458797,
    251257782,
    7010936175,
    2987814177,
    5298673192,
    1129946922,
    7313361615,
    7429544958,
    11925856,
    1926195282
}

local function checkPlayer()
    for _, id in ipairs(allowedUserIds) do
        if player.UserId == id then
            return true
        end
    end
    player:Kick("Braxy and Araa did not allow you to use this script")
    return false
end

if not checkPlayer() then
    return
end


local function ZYZZS_fake_script() -- Reputation.LocalScript 
	local script = Instance.new('LocalScript', Reputation)

	local function formatNumber(number)
		local formatted = tostring(number)
	
		formatted = string.reverse(formatted)
		formatted = string.gsub(formatted, "(%d%d%d)", "%1,")
	
		formatted = string.reverse(formatted)
		formatted = string.gsub(formatted, "^,", "")
	
		return formatted
	end
	
	local function updateText()
		local player = game.Players.LocalPlayer
		if player then
			local reputationValue = player.leaderstats.Reputation.Value
			script.Parent.Text = " " .. formatNumber(reputationValue)
		end
	end
	
	updateText()
	
	local player = game.Players.LocalPlayer
	if player then
		player.leaderstats.Reputation:GetPropertyChangedSignal("Value"):Connect(function()
			updateText()
		end)
	end
	
end
coroutine.wrap(ZYZZS_fake_script)()
local function OGQQZAG_fake_script() -- Ninjutsu.LocalScript 
	local script = Instance.new('LocalScript', Ninjutsu)

	local function formatNumber(number)
		local formatted = tostring(number)
	
		formatted = string.reverse(formatted)
		formatted = string.gsub(formatted, "(%d%d%d)", "%1,")
	
		formatted = string.reverse(formatted)
		formatted = string.gsub(formatted, "^,", "")
	
		return formatted
	end
	
	local function updateText()
		local player = game.Players.LocalPlayer
		if player then
			local ninjutsuValue = player.leaderstats.Ninjutsu.Value
			script.Parent.Text = " " .. formatNumber(ninjutsuValue)
		end
	end
	
	updateText()
	
	local player = game.Players.LocalPlayer
	if player then
		player.leaderstats.Ninjutsu:GetPropertyChangedSignal("Value"):Connect(function()
			updateText()
		end)
	end
	
end
coroutine.wrap(OGQQZAG_fake_script)()

local StarterGui = game:GetService("StarterGui")

local function sendNotification(title, text)
	local notification = {
		Title = title,
		Text = text,
		Duration = 10000000,
		Button1 = "Close", -- Button text
		Callback = function(action)
			if action == "Button1" then
				print("Close button pressed")
			end
		end
	}

	StarterGui:SetCore("SendNotification", notification)
end

sendNotification("HunterHub", "Welcome to HunterHub Gui")

-- LocalScript (place it in StarterPlayerScripts or StarterCharacterScripts)

-- NA PC Version script:

loadstring(game:HttpGet('https://raw.githubusercontent.com/LoneWalker0922/YIN-YANG-NA/main/NAmobilefix.txt'))()
