AutoRespawn.Name = "AutoRespawn"
AutoRespawn.Parent = TextLabel_12
AutoRespawn.BackgroundColor3 = Color3.fromRGB(84, 84, 84)
AutoRespawn.BorderColor3 = Color3.fromRGB(139, 139, 139)
AutoRespawn.Position = UDim2.new(1, 0, 0, 0)
AutoRespawn.Size = UDim2.new(0.315315336, 0, 1, 0)
AutoRespawn.Font = Enum.Font.Merriweather
AutoRespawn.Text = "Exct"
AutoRespawn.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoRespawn.TextScaled = true
AutoRespawn.TextSize = 14.000
AutoRespawn.TextWrapped = true
AutoRespawn.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer

	local lastPosition = nil

	local function respawnCharacter()
		if player.Character and player.Character.PrimaryPart then
			lastPosition = player.Character.PrimaryPart.Position
			player.Character:Destroy()
		end
		game:GetService("ReplicatedStorage").RemoteEvent.SpawnCharacterEvent:FireServer("MainSpawn")
	end

	local function connectDiedEvent()
		if player.Character then
			local humanoid = player.Character:WaitForChild("Humanoid")
			humanoid.Died:Connect(respawnCharacter)
			humanoid.HealthChanged:Connect(function(health)
				if health <= 0.3 * humanoid.MaxHealth then
					respawnCharacter()
				end
			end)
		end
	end

	player.CharacterAdded:Connect(function(character)
		connectDiedEvent()
		character:WaitForChild("HumanoidRootPart")
		if lastPosition then
			character:SetPrimaryPartCFrame(CFrame.new(lastPosition))
			lastPosition = nil
		end
	end)
end)
	
