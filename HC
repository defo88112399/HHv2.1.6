KillPlayer.Name = "KillPlayer"
KillPlayer.Parent = TextLabel_7
KillPlayer.BackgroundColor3 = Color3.fromRGB(84, 84, 84)
KillPlayer.BorderColor3 = Color3.fromRGB(139, 139, 139)
KillPlayer.Position = UDim2.new(1, 0, 0, 0)
