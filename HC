NoFF. MouseButton1Click:Connect(function()
	local function equipSword()
		-- Wait until the Sword is in the backpack
		repeat
			wait()
		until game.Players.LocalPlayer.Backpack:FindFirstChild("Sword")

		-- Move the Sword from backpack to character
		local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Sword")
		if tool then
			tool.Parent = game.Players.LocalPlayer.Character
		end
	end

	local function unequipSword()
		local sword = game.Players.LocalPlayer.Character:FindFirstChild("Sword")
		if sword then
			sword.Parent = game.Players.LocalPlayer.Backpack
		end
	end

	while true do
		wait(0.1) -- Adjust the wait time here

		local character = game.Players.LocalPlayer.Character
		if character and character:FindFirstChild("ForceField") then
			-- Forcefield detected, perform the action
			equipSword()

			-- Wait a moment
			wait(0.1) -- Adjust this delay if necessary

			-- Unequip the sword
			unequipSword()
		end
	end
end)

