KillPlayer.TextColor3 = Color3.fromRGB(0, 0, 0)
KillPlayer.TextScaled = true
KillPlayer.TextSize = 14.000
KillPlayer.TextWrapped = true
KillPlayer.MouseButton1Click:Connect(function()
	-- Function to fire a shuriken at a target player
	local function fireShuriken(targetPlayer)
		if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") or not targetPlayer.Character:FindFirstChild("Head") then
			warn("Invalid target player")
			return
		end

		local targetPosition = targetPlayer.Character.Head.Position
		game.Players.LocalPlayer.Character.Shuriken.Handle.CFrame = CFrame.new(targetPosition) -- Teleport shuriken to target's head
		game.Players.LocalPlayer.Character.Shuriken.HitEvent:FireServer(targetPosition)
	end

	-- Create the player selector GUI
	local gui = Instance.new("ScreenGui")
	gui.Name = "PlayerSelectorGUI"
	gui.ResetOnSpawn = false -- Ensure GUI persists after respawn

	-- Create a frame to contain the player buttons
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(0, 200, 0, 300)
	frame.Position = UDim2.new(0.5, -100, 0.5, -150)
	frame.BackgroundColor3 = Color3.new(0, 0, 0)
	frame.BorderSizePixel = 2
	frame.Visible = false -- Initially hide the frame
	frame.Parent = gui

	-- Create a scrolling frame to contain the player buttons
	local scrollFrame = Instance.new("ScrollingFrame")
	scrollFrame.Size = UDim2.new(1, 0, 1, -30) -- Adjusted size to accommodate label
	scrollFrame.Position = UDim2.new(0, 0, 0, 30) -- Adjusted position to leave space for label
	scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
	scrollFrame.BackgroundTransparency = 1
	scrollFrame.Parent = frame

	-- Function to make an element draggable
	local function makeDraggable(element)
		local dragging
		local dragStart
		local startPos

		element.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = element.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		element.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
				local delta = input.Position - dragStart
				element.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
			end
		end)
	end

	-- Create the label
	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 0, 30) -- Adjusted size for label
	label.Position = UDim2.new(0, 0, 0, 0)
	label.BackgroundColor3 = Color3.new(0, 0, 0)
	label.Text = "LoneWalkerHub"
	label.TextColor3 = Color3.new(1, 1, 1)
	label.TextSize = 18
	label.Parent = frame

	-- Make the frame draggable instead of the label
	makeDraggable(frame)

	-- Variable to store the selected player
	local selectedPlayer = nil

	-- Function to continuously fire shurikens at the selected player
	local function continuouslyFireShuriken()
		while selectedPlayer do
			fireShuriken(selectedPlayer)
			wait(1 / 700) -- Adjust the interval between firing based on shuriken speed
		end
	end

	-- Function to add a player to the player list
	local function addPlayerToList(player)
		local buttonHeight = 20
		local padding = 5
		local offsetY = (#scrollFrame:GetChildren() - 1) * (buttonHeight + padding)

		local button = Instance.new("TextButton")
		button.Size = UDim2.new(1, -padding * 2, 0, buttonHeight)
		button.Position = UDim2.new(0, padding, 0, offsetY)
		button.Text = player.Name
		button.TextSize = 14
		button.Name = player.Name .. "Button"
		button.Parent = scrollFrame

		-- Event handler for button click
		button.MouseButton1Click:Connect(function()
			selectedPlayer = player -- Set the selected player
			continuouslyFireShuriken() -- Start continuous firing
		end)

		scrollFrame.CanvasSize = UDim2.new(0, 0, 0, offsetY + buttonHeight + padding)
	end

	-- Function to remove a player from the player list
	local function removePlayerFromList(player)
		local button = scrollFrame:FindFirstChild(player.Name .. "Button")
		if button then
			button:Destroy()

			-- Recalculate the positions of remaining buttons
			local buttons = scrollFrame:GetChildren()
			local buttonHeight = 20
			local padding = 5
			local offsetY = 0

			for _, btn in ipairs(buttons) do
				if btn:IsA("TextButton") then
					btn.Position = UDim2.new(0, padding, 0, offsetY)
					offsetY = offsetY + buttonHeight + padding
				end
			end

			scrollFrame.CanvasSize = UDim2.new(0, 0, 0, offsetY)
		end
	end

	-- Function to populate the player list
	local function populatePlayerList()
		local players = game.Players:GetPlayers()
		for _, player in ipairs(players) do
			addPlayerToList(player)
		end
	end

	-- Add event listener for new players joining
	game.Players.PlayerAdded:Connect(addPlayerToList)

	-- Add event listener for players leaving
	game.Players.PlayerRemoving:Connect(removePlayerFromList)

	-- Call the function to populate the player list initially
	populatePlayerList()

	-- Add open/close button
	local openCloseButton = Instance.new("TextButton")
	openCloseButton.Size = UDim2.new(0, 83, 0, 23) -- Adjusted size for button
	openCloseButton.Position = UDim2.new(0.879, 0, 0.663, 0) -- Adjusted position for button
	openCloseButton.Text = "Open Gui" -- Adjusted text for button
	openCloseButton.TextSize = 14 -- Adjusted text size for button
	openCloseButton.Parent = gui
	openCloseButton.Active = true
	openCloseButton.Visible = true

	local isOpen = false

	openCloseButton.MouseButton1Click:Connect(function()
		if isOpen then
			frame.Visible = false
			isOpen = false
			openCloseButton.Text = "Open Gui" -- Adjusted text for button
		else
			frame.Visible = true
			isOpen = true
			openCloseButton.Text = "Close Gui" -- Adjusted text for button
		end
	end)

	-- Function to reparent GUI to PlayerGui
	local function reparentGUI()
		gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	end

	-- Initial GUI parent
	reparentGUI()

	-- Ensure GUI persists after respawn
	game.Players.LocalPlayer.CharacterAdded:Connect(reparentGUI)
end)

