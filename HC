AutoDropTS.Name = "AutoDropTS"
AutoDropTS.Parent = TextLabel_4
AutoDropTS.BackgroundColor3 = Color3.fromRGB(84, 84, 84)
AutoDropTS.BorderColor3 = Color3.fromRGB(139, 139, 139)
AutoDropTS.Position = UDim2.new(1, 0, 0, 0)
AutoDropTS.Size = UDim2.new(0.315315336, 0, 1, 0)
AutoDropTS.Font = Enum.Font.Merriweather
AutoDropTS.Text = "Exct"
AutoDropTS.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoDropTS.TextScaled = true
AutoDropTS.TextSize = 14.000
AutoDropTS.TextWrapped = true
AutoDropTS.MouseButton1Click:Connect(function()
	local Players = game:GetService("Players")

	-- Function to drop a tool
	local function dropTool(tool)
		if tool and tool:IsA("Tool") then
			tool.Parent = workspace
			tool.Handle.CFrame = game.Players.LocalPlayer.Character.PrimaryPart.CFrame * CFrame.new(0, -5, 0) -- Drop the tool below the character
		end
	end

	-- Function to handle the respawn action
	local function onPlayerRespawn()
		-- Add a delay of 1 second
		wait(1)

		-- Assuming you have a tool named "Train" in the backpack
		local player = game.Players.LocalPlayer
		local backpack = player:FindFirstChild("Backpack")
		local trainTool = backpack and backpack:FindFirstChild("Train")

		if trainTool then
			-- Equip the tool to the character
			trainTool.Parent = player.Character

			-- Wait for a brief moment (optional)
			wait(0.2)

			-- Drop the tool
			dropTool(trainTool)
		end
	end

	-- Function to connect the respawn event
	local function setupPlayerRespawn()
		local player = Players.LocalPlayer

		-- Function to handle setup and initial respawn
		local function setupAndRespawn()
			-- Call onPlayerRespawn immediately
			onPlayerRespawn()

			-- Connect to the PlayerAdded event (for initial spawn)
			player.CharacterAdded:Connect(function(character)
				onPlayerRespawn()
			end)

			-- Connect to the ChildAdded event (for respawn after death)
			player.ChildAdded:Connect(function(child)
				if child:IsA("Model") and child.Name == "Player" then
					onPlayerRespawn()
				end
			end)
		end

		-- Check if LocalPlayer is already present
		if player.Character then
			setupAndRespawn()
		else
			-- Wait for the LocalPlayer to be added
			player.CharacterAdded:Connect(setupAndRespawn)
		end
	end

	-- Call setupPlayerRespawn initially
	setupPlayerRespawn()
end)

