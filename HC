-- Scripts:

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerKicked = false

local function checkNinjutsu()
    if playerKicked then return end

    local leaderstats = player:FindFirstChild("leaderstats")
    if leaderstats then
        local ninjutsu = leaderstats:FindFirstChild("Ninjutsu")
        if ninjutsu and ninjutsu:IsA("IntValue") then
            if ninjutsu.Value < 1000 then
                playerKicked = true
                player:Kick("Your Ninjutsu is too low.")
                return
            end
        end
    end
end

player.CharacterAdded:Connect(checkNinjutsu)

if player.Character then
    checkNinjutsu()
end

player.CharacterAdded:Connect(checkNinjutsu)

if player.Character then
    checkNinjutsu()
end

local player = game.Players.LocalPlayer

local allowedUserIds = {
    3056635216,
    3839931017,
    3241244580,
    343620144,
    2055703494,
    443203200,
    3955749588,
    4460596274,
    3413764023,
    3437132949,
    5818650558,
    5502052653,
    2735237971,
    6082023906,
    776995552,
    6113265215,
    3697296786,
    6107794853,
    5844502462,
    3453880755,
    471563983,
    1926195282,
    7142414152,
    5050633334,
    1516428850,
